<Project>

  <PropertyGroup>
    <DotNetCoreCommon>3.1.7</DotNetCoreCommon>
    <AspNetCommon>5.2.7</AspNetCommon>
  </PropertyGroup>

  <ItemGroup Label="Microsoft.Extensions">
    <PackageReference Update="Microsoft.Extensions.DependencyInjection.Abstractions" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.DependencyInjection" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Configuration" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.Binder" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.FileExtensions" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Options" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.Binder" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.Json" Version="$(DotNetCoreCommon)" />
    <PackageReference Update="Microsoft.Extensions.Logging" Version="$(DotNetCoreCommon)" />
  </ItemGroup>

  <ItemGroup Label="Microsoft.Extensions">
    <PackageReference Update="Microsoft.AspNet.Mvc" Version="$(AspNetCommon)" />
    <PackageReference Update="Microsoft.AspNet.WebApi" Version="$(AspNetCommon)" />
    <PackageReference Update="Microsoft.AspNet.WebApi.WebHost" Version="$(AspNetCommon)" />
    <PackageReference Update="Microsoft.AspNet.WebApi.Core" Version="$(AspNetCommon)" />
  </ItemGroup>

  <ItemGroup Label="System">
    <PackageReference Update="System.Configuration.ConfigurationManager" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup Label="Microsoft.FeatureManagement">
    <PackageReference Update="Azure.Identity" Version="1.1.1" />
    <PackageReference Update="Microsoft.Extensions.Configuration.AzureAppConfiguration" Version="3.0.2" />
    <PackageReference Update="Microsoft.FeatureManagement" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup Label="Owin">
    <PackageReference Update="Microsoft.Owin" Version="4.1.0" />
    <PackageReference Update="Microsoft.Owin.Host.SystemWeb" Version="4.1.0" />
    <PackageReference Update="Owin" Version="1.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Bet.Extensions.AzureAppConfiguration" Version="3.1.7" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="coverlet.collector" Version="1.3.0" />
    <PackageReference Update="Microsoft.NET.Test.Sdk" Version="16.7.0" />
    <PackageReference Update="xunit" Version="2.4.1" />
    <PackageReference Update="xunit.runner.visualstudio" Version="2.4.3" />
  </ItemGroup>


  <ItemGroup Label="SourceLink">
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup Label="Analyzers">
    <PackageReference Include="Bet.CodeAnalyzers" Version="1.0.10">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Bet.EditorConfig" Version="1.0.10">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <None Include="../../img/icon.png" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>

</Project>
