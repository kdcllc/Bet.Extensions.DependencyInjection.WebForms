<Project>

  <PropertyGroup Label="Basic Settings">
    <VersionPrefix>1.0.1-preview1</VersionPrefix>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
    <LangVersion>latest</LangVersion>
    <Nullable>disable</Nullable>
    <NoWarn>$(NoWarn);CS1591;NU1605;</NoWarn>
  </PropertyGroup>

  <PropertyGroup Label="Ownership Settings">
    <Company>King David Consulting LLC</Company>
    <Authors>kdcllc</Authors>
    <Copyright>Copyright Â© King David Consulting LLC</Copyright>
  </PropertyGroup>

  <PropertyGroup Label="Nuget Package Settings">
    <Description>Collection of Migration Tools for .NET Framework Applications to AspNetCore or DotNetCore applications.</Description>
    <PackageProjectUrl>https://github.com/kdcllc/Bet.Extensions.DependencyInjection.WebForms</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kdcllc/Bet.Extensions.DependencyInjection.WebForms.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicense>https://github.com/kdcllc/Bet.Extensions.DependencyInjection.WebForms/master/LICENSE</PackageLicense>

    <!-- https://docs.microsoft.com/en-us/nuget/reference/errors-and-warnings/nu5048 -->
    <PackageIcon>icon.png</PackageIcon>

    <PackageTags>Migration, AspNetCore, DotNetCore, Configuration, Hosting, Extensions</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Label="SourceLink Settings">
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>

    <DebugType>Embedded</DebugType>
    <EmbedAllSources>True</EmbedAllSources>
  </PropertyGroup>
</Project>
